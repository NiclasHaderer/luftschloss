{
  "name": "luftschloss",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "pwd": "pwd",
    "build": "turbo run build",
    "build:w": "yarn workspaces foreach -ti --exclude luftschloss run build && yarn workspaces foreach -pi --exclude luftschloss run build -w",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "tslint": "^6.1.3",
    "turbo": "^1.1.6",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "packageManager": "yarn@3.2.0"
}
